{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prefer-const": [
            "warn",
            {
              "destructuring": "all",
              "ignoreReadBeforeAssign": false
            }
          ]
    }
}
